    "@graphql-typed-document-node/core": "^3.1.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "fix-contentful-schema": "^1.0.0",
    "nodemon": "2.0.7",
    "prisma": "~2.23.0",
    "tslint": "6.1.3",
