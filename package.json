    "@types/node": "^14.14.35",
    "apollo-server": "^2.25.3",
    "aws-sdk": "^2.814.0",
    "class-validator": "^0.13.0",
    "dotenv": "^9.0.0",
    "express": "^4.17.3",
    "firebase-admin": "^9.5.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.8.0",
    "mailgun.js": "^3.4.0",
    "marked": "^4.0.10",
    "marked-plaintext": "^0.0.2",
    "multer": "^1.4.2",
    "nanoid": "^3.1.31",
